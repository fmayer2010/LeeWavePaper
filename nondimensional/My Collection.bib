isbn = {0924-7963},
issn = {09247963},
journal = {Journal of Marine Systems},
keywords = {Boundary conditions,Data assimilation,GODAE,HYCOM,Ocean modeling,Ocean prediction},
number = {1-4 SPEC. ISS.},
