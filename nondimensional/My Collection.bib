doi = {10.1175/1520-0485(1993)023<1346:GOMFAJ>2.0.CO;2},
file = {:Users/frederickmayer/Library/Application Support/Mendeley Desktop/Downloaded/Vallis, Maltrud - 1993 - Generation of Mean Flows and Jets on a Beta Plane and over Topography.pdf:pdf},
issn = {0022-3670},
pages = {1346--1362},
title = {{Generation of Mean Flows and Jets on a Beta Plane and over Topography}},
volume = {23},
year = {1993}
