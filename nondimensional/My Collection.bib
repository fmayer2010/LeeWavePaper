journal = {Journal of Physical Oceanography},
keywords = {Anticyclones,Boundary currents,Circulation/ Dynamics,Coastal flows,Mixing,Orographic effects,Small scale processes},
number = {3},
pages = {613--629},
pmid = {90671844},
title = {{Submesoscale Instability and Generation of Mesoscale Anticyclones near a Separation of the California Undercurrent}},
url = {http://journals.ametsoc.org/doi/10.1175/JPO-D-13-0225.1},
