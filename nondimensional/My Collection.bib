when N≈10 ?2 s ?1},
author = {Thorpe, S A},
booktitle = {Journal of Physical Oceanography},
doi = {10.1175/1520-0485(1996)026<0191:TCSTOM>2.0.CO;2},
file = {:Users/frederickmayer/Library/Application Support/Mendeley Desktop/Downloaded/Thorpe - 1996 - The Cross-Slope Transport of Momentum by Internal Waves Generated by Alongslope Currents over Topography.pdf:pdf},
issn = {0022-3670},
number = {2},
pages = {191--204},
title = {{The Cross-Slope Transport of Momentum by Internal Waves Generated by Alongslope Currents over Topography}},
url = {http://dx.doi.org/10.1175/1520-0485(1996)026{\%}3C0191:TCSTOM{\%}3E2.0.CO{\%}5Cn2},
