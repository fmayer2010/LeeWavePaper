journal = {Journal of Geophysical Research: Oceans},
keywords = {http://dx.doi.org/10.1029/2006JC003879, doi:10.102},
number = {5},
pages = {1--14},
title = {{Global estimates of seafloor slope from single-beam ship soundings}},
volume = {113},
year = {2008}
}
@article{Bauer2000,
abstract = {The influence of the obstacle shape, expressed through the ratio of$\backslash$nspanwise to streamwise extension beta, on flow over and around a$\backslash$nmesoscale mountain is examined numerically. The initial wind U as$\backslash$nwell as the buoyancy frequency N are constant; the earth's rotation$\backslash$nand surface friction are neglected. In these conditions the flow$\backslash$nresponse depends primarily on the nondimensional mountain height$\backslash$nH-m = h(m)N/U (where h(m) is the maximum mountain height) and the$\backslash$nhorizontal aspect ratio b. A regime diagram for the onset of wave$\backslash$nbreaking, lee vortex formation, and windward stagnation is compiled.$\backslash$nWhen beta is increased, smaller H-m are required for the occurrence$\backslash$nof all three features. It is demonstrated that lee vortices can form$\backslash$nwith neither wave breaking above the lee slope nor upstream stagnation.$\backslash$nFor beta less than or equal to 0.5 a vortex pair can appear although$\backslash$nthe isentropes above the lee slope do not overturn for any H-m. For$\backslash$nbeta{\textgreater} 1, on the other hand, lee vortex formation is triggered by$\backslash$nwave breaking. On the windward side two distinct processes can lead$\backslash$nto a complete blocking of the flow: the piling up of heavier air$\backslash$nahead of the barrier and the upstream propagation of columnar modes,$\backslash$nwhich are generated by the wave breaking process for beta{\textgreater} 1. "High-drag''$\backslash$nstates and "downslope windstorms'' exist above the threshold of wave$\backslash$nbreaking as long as no lee vortices appear (or, at least, as long$\backslash$nas they are very small). Hence, the interval of H-m where a high-drag$\backslash$nstate occurs becomes progressively larger for larger beta. With the$\backslash$ngrowth of lee vortices the maximum wind speed along the leeward slope$\backslash$nis dampened. The normalized drag drops rapidly below its linear counterpart$\backslash$nand asymptotically approaches zero.},
author = {Bauer, Manfred H and Mayr, Georg J and Vergeiner, Ignaz and Pichler, Helmut},
doi = {10.1175/1520-0469(2001)058<3971:SNFOAA>2.0.CO;2},
file = {:Users/frederickmayer/Desktop/Stanford/PhD/reading list/Bauer{\_}et{\_}al{\_}2000.pdf:pdf},
isbn = {0022-4928},
issn = {15200469},
journal = {Journal of the Atmospheric Sciences},
number = {24},
pages = {3971--3991},
title = {{Strongly Nonlinear Flow over and around a Three-Dimensional Mountain as a Function of the Horizontal Aspect Ratio}},
url = {{\%}5C{\%}5CIBSERVER{\%}5CID{\_}enl{\%}5CJournal{\_}of{\_}the{\_}Atmospheric{\_}Sciences{\%}5C2000{\%}5C057-24-3971.pdf},
volume = {57},
